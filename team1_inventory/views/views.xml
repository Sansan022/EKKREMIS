<odoo>
  <data>

  <record id="stock_view_picking_form" model="ir.ui.view">
    <field name="name">stock.picking.view.form.inherit</field>
    <field name="model">stock.picking</field>
    <field name="inherit_id" ref="stock.view_picking_form"/>
    <field name="arch" type="xml">

      <field name="move_type" position="attributes">
        <attribute name="invisible">True</attribute>
      </field>

      <field name="group_id" position="attributes">
        <attribute name="invisible">True</attribute>
      </field>
      <field name="priority" position="attributes">
        <attribute name="invisible">True</attribute>
      </field>
     
      <field name="partner_id" position="attributes">
        <attribute name="string">Subscriber</attribute>
      </field>


      <field name="min_date" position="attributes">
        <attribute name="string">Service Date</attribute>
      </field>

    </field>
  </record>



  <record id="product_template_form_view" model="ir.ui.view">
    <field name="name">product.template.view.form.inherit</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="arch" type="xml">

     
      <xpath expr="//button[@name='action_view_orderpoints']" position="attributes">
        <attribute name="invisible">1</attribute>
     </xpath>
   
    <xpath expr="//button[@name='action_view_sales']" position="attributes">
          <attribute name="invisible">1</attribute>
      </xpath>   
    

    </field>
  </record>  




